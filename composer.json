{
    "name": "addispay/addispay-sdk",
    "description": "Laravel SDK for AddisPay payment gateway",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashenafi Alemu",
            "email": "ashenafialemu27@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AddisPay\\AddisPaySDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AddisPay\\AddisPaySDK\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AddisPay\\AddisPaySDK\\Providers\\AddisPayServiceProvider"
            ],
            "aliases": {
                "AddisPay": "AddisPay\\AddisPaySDK\\Facades\\AddisPay"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*",
        "mockery/mockery": "*"
    }
}
